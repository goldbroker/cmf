{
    "name": "goldbroker/cmf",
    "type": "symfony-bundle",
    "description": "GoldBroker CMF - a fork & merge of abandoned symfony-cmf packages",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "doctrine/annotations": "^1.4 || ^2.0",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0",
        "friendsofsymfony/rest-bundle": "^2.7 || ^3.0",
        "helios-ag/fm-elfinder-bundle": "^10.0 || ^11.0 || ^12.0",
        "imagine/imagine": "^1.3",
        "jms/serializer-bundle": "^4.0",
        "knplabs/gaufrette": "^0.10.0",
        "knplabs/knp-menu-bundle": "^2.2 || ^3.0",
        "knplabs/knp-menu": "^2.0 || ^3.0",
        "sonata-project/admin-bundle": "^4.0",
        "sonata-project/block-bundle": "^4.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/filesystem": "^5.4",
        "symfony/finder": "^5.4 || ^6.0",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/framework-bundle": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/options-resolver": "^5.4 || ^6.0",
        "symfony/property-access": "^5.4 || ^6.0",
        "symfony/security-core": "^5.4 || ^6.0",
        "symfony/templating": "^5.4 || ^6.0",
        "symfony/twig-bundle": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony-cmf/routing-bundle": "^2.0 || ^3.0",
        "webmozart/assert": "^1.2",
        "knplabs/doctrine-behaviors": "^2.2",
        "sonata-project/translation-bundle": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.0.6",
        "burgov/key-value-form-bundle": "^1.0",
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "doctrine/orm": "^2.7",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "imbo/behat-api-extension": "^2.1",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "jackalope/jackalope-fs": "^0.0.4",
        "liip/imagine-bundle": "^2.12",
        "matthiasnoback/symfony-config-test": "^4",
        "matthiasnoback/symfony-dependency-injection-test": "^4",
        "mikey179/vfsstream": "^1.2",
        "phpspec/prophecy": "^1.12",
        "sonata-project/form-extensions": "^1.6",
        "sonata-project/seo-bundle": "^3.0",
        "symfony/asset": "^5.4 || ^6.0",
        "symfony/css-selector": "^5.4 || ^6.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/security-bundle": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0",
        "symfony/yaml": "^5.4 || ^6.0",
        "symfony-cmf/testing": "*",
        "twig/twig": "^1.14.2 || ^2.0",
        "dg/bypass-finals": "^1.8",
        "symfony/polyfill-intl-icu": "^1.21",
        "doctrine/cache": "^1.12",
        "sonata-project/twig-extensions": "^1.12"
    },
    "conflict": {
        "jms/serializer-bundle": "<2.3.1",
        "sonata/cache": "<1.1.1",
        "sonata-project/doctrine-extensions": "<=1.3.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "replace": {
        "symfony-cmf/block-bundle": "*",
        "symfony-cmf/content-bundle": "*",
        "symfony-cmf/core-bundle": "*",
        "symfony-cmf/media-bundle": "*",
        "symfony-cmf/menu-bundle": "*",
        "symfony-cmf/resource": "*",
        "symfony-cmf/resource-bundle": "*",
        "symfony-cmf/resource-rest-bundle": "*",
        "symfony-cmf/seo-bundle": "*",
        "symfony-cmf/sonata-phpcr-admin-integration-bundle": "*",
        "symfony-cmf/tree-browser-bundle": "*",
        "m4nu/multi-domain-bundle": "*",
        "sonata-project/doctrine-phpcr-admin-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\": "src/Bundle/",
            "Symfony\\Cmf\\Component\\": "src/Component/",
            "Sonata\\": "src/Sonata/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
