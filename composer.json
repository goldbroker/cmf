{
    "name": "goldbroker/cmf",
    "version": "1.1.0",
    "type": "symfony-bundle",
    "description": "GoldBroker CMF - a fork & merge of abandoned symfony-cmf packages",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8",
        "doctrine/phpcr-bundle": "^1.3 || ^2.0",
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0",
        "knplabs/knp-menu-bundle": "^2.2 || ^3.0",
        "knplabs/knp-menu": "^2.0 || ^3.0",
        "jms/serializer-bundle": "^2.0 || ^3.0",
        "sonata-project/block-bundle": "^3.11 || ^4",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/options-resolver": "^4.4 || ^5.0 || ^6.0",
        "symfony/templating": "^4.4 || ^5.0 || ^6.0",
        "symfony/twig-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/security-core": "^4.4 || ^5.0 || ^6.0",
        "symfony/validator": "^4.4 || ^5.0 || ^6.0",
        "webmozart/assert": "^1.2",
        "webmozart/path-util": "^2.3",
        "helios-ag/fm-elfinder-bundle": "^10.0.4",
        "knplabs/gaufrette": "^0.10.0",
        "imagine/imagine": "^1.3"
    },
    "require-dev": {
        "behat/behat": "^3.0.6",
        "imbo/behat-api-extension": "^2.1",
        "jackalope/jackalope-doctrine-dbal": "^1.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4",
        "matthiasnoback/symfony-config-test": "^4",
        "mikey179/vfsstream": "^1.2",
        "phpspec/prophecy": "^1.12",
        "sonata-project/admin-bundle": "^3.1 || ^4",
        "symfony/asset": "^4.4 || ^5.0 || ^6.0",
        "symfony/form": "^4.4 || ^5.0 || ^6.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/security-bundle": "^4.4 || ^5.0 || ^6.0",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0",
        "symfony-cmf/testing": "*",
        "twig/twig": "^1.14.2 || ^2.0",
        "jackalope/jackalope-fs": "^0.0.4",
        "symfony-cmf/routing-bundle": "^2.5",
        "mockery/mockery": "^1.6",
        "doctrine/doctrine-bundle": "^2.12",
        "symfony/css-selector": "^4.4 || ^5.0 || ^6.0",
        "liip/imagine-bundle": "^2.12"
    },
    "replace": {
        "symfony-cmf/block-bundle": "*",
        "symfony-cmf/core-bundle": "*",
        "symfony-cmf/content-bundle": "*",
        "symfony-cmf/media-bundle": "*",
        "symfony-cmf/menu-bundle": "*",
        "symfony-cmf/resource": "*",
        "symfony-cmf/resource-bundle": "*",
        "symfony-cmf/resource-rest-bundle": "*",
        "symfony-cmf/tree-browser-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\BlockBundle\\": "src/Bundle/BlockBundle/",
            "Symfony\\Cmf\\Bundle\\ContentBundle\\": "src/Bundle/ContentBundle/",
            "Symfony\\Cmf\\Bundle\\CoreBundle\\": "src/Bundle/CoreBundle/",
            "Symfony\\Cmf\\Bundle\\MediaBundle\\": "src/Bundle/MediaBundle/",
            "Symfony\\Cmf\\Bundle\\MenuBundle\\": "src/Bundle/MenuBundle/",
            "Symfony\\Cmf\\Bundle\\ResourceBundle\\": "src/Bundle/ResourceBundle/",
            "Symfony\\Cmf\\Bundle\\ResourceRestBundle\\": "src/Bundle/ResourceRestBundle/",
            "Symfony\\Cmf\\Bundle\\TreeBrowserBundle\\": "src/Bundle/TreeBrowserBundle/",
            "Symfony\\Cmf\\Component\\": "src/Component/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Symfony\\Cmf\\Bundle\\BlockBundle\\": "tests/Bundle/BlockBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\ContentBundle\\": "tests/Bundle/ContentBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\CoreBundle\\": "tests/Bundle/CoreBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\MediaBundle\\": "tests/Bundle/MediaBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\MenuBundle\\": "tests/Bundle/MenuBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\ResourceBundle\\": "tests/Bundle/ResourceBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\ResourceRestBundle\\": "tests/Bundle/ResourceRestBundle/",
            "Tests\\Symfony\\Cmf\\Bundle\\TreeBrowserBundle\\": "tests/Bundle/TreeBrowserBundle/",
            "Tests\\Symfony\\Cmf\\Component\\": "tests/Component/"
        }
    },
    "conflict": {
        "sonata/cache": "<1.1.1"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
