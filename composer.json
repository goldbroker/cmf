{
    "name": "goldbroker/cmf",
    "type": "symfony-bundle",
    "description": "GoldBroker CMF - a fork & merge of abandoned symfony-cmf packages",
    "keywords": [
        "Symfony CMF",
        "Block",
        "content fragments"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "doctrine/phpcr-bundle": "^2.0 || ^3.0",
        "doctrine/phpcr-odm": "^1.4.2 || ^2.0 || ^3.0.x-dev",
        "friendsofsymfony/rest-bundle": "^2.7 || ^3.0",
        "helios-ag/fm-elfinder-bundle": "^10.0 || ^11.0 || ^12.0",
        "imagine/imagine": "^1.3",
        "jms/serializer-bundle": "^4.0 || ^5.0",
        "knplabs/doctrine-behaviors": "^2.2 || dev-sf7.0 as 2.6.3",
        "knplabs/gaufrette": "^0.10 || ^0.11",
        "knplabs/knp-menu-bundle": "^2.2 || ^3.0",
        "knplabs/knp-menu": "^2.0 || ^3.0",
        "sonata-project/admin-bundle": "^4.39 || ^5.0",
        "sonata-project/block-bundle": "^4.0 || ^5.0",
        "sonata-project/translation-bundle": "^3.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/filesystem": "^6.4 || ^7.0",
        "symfony/finder": "^6.4 || ^7.0",
        "symfony/form": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-foundation": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/options-resolver": "^6.4 || ^7.0",
        "symfony/property-access": "^6.4 || ^7.0",
        "symfony/security-core": "^6.4 || ^7.0",
        "symfony/templating": "^6.4 || ^7.0",
        "symfony/twig-bundle": "^6.4 || ^7.0",
        "symfony/validator": "^6.4 || ^7.0",
        "symfony-cmf/routing-bundle": "^2.0 || ^3.0",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.2"
    },
    "conflict": {
        "jms/serializer-bundle": "<2.3.1",
        "sonata/cache": "<1.1.1",
        "sonata-project/doctrine-extensions": "<=1.3.0"
    },
    "provide": {
        "sonata-project/admin-bundle-persistency-layer": "1.0.0"
    },
    "replace": {
        "symfony-cmf/block-bundle": "*",
        "symfony-cmf/content-bundle": "*",
        "symfony-cmf/core-bundle": "*",
        "symfony-cmf/media-bundle": "*",
        "symfony-cmf/menu-bundle": "*",
        "symfony-cmf/resource": "*",
        "symfony-cmf/resource-bundle": "*",
        "symfony-cmf/resource-rest-bundle": "*",
        "symfony-cmf/seo-bundle": "*",
        "symfony-cmf/sonata-phpcr-admin-integration-bundle": "*",
        "symfony-cmf/tree-browser-bundle": "*",
        "m4nu/multi-domain-bundle": "*",
        "sonata-project/doctrine-phpcr-admin-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Cmf\\Bundle\\": "src/Bundle/",
            "Symfony\\Cmf\\Component\\": "src/Component/",
            "Sonata\\": "src/Sonata/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "repositories": {
        "jfsenechal_doctrine_behaviors": {
            "type": "vcs",
            "url": "git@github.com:jfsenechal/DoctrineBehaviors.git"
        }
    }
}
